<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chanys.assignment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuActivity"
            android:label="Menu"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SportActivity"
            android:label="Sport"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".BookingActivity"
            android:label="Booking"
            android:parentActivityName=".SportActivity" />
        <activity
            android:name=".BookingConfirmationActivity"
            android:label="Confirmarion"
            android:parentActivityName=".BookingActivity"></activity>
    </application>

</manifest>